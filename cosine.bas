5 REM DEMONSTRATE TAB FUNCTION, PAGE 33
10 PRINT "THE GRAPH OF COSINE(X) FOR X = 0 TO 180";
15 PRINT " IN STEPS OF 15 DEGREES"
20 PRINT "X";TAB(62);"COS(X)"
30 FOR X=0 TO 180 STEP 15
40 LET C=COS(X * 3.141592 / 180)
50 LET P=INT(30 * ABS(C) + .5) * SGN(C)
60 PRINT X; TAB(P + 36); "*"
70 NEXT X
80 END
